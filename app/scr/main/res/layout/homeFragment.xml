<?xml version="1.0" encoding="UTF-8"?>

<TextView
        android:id="@+id/homeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginEnd="39dp"
        android:alpha="1"
        android:text="Wellcome to dionstics"
        android:textColor="#FF0202"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="HardcodedText,MissingConstraints,UnusedAttribute"
        tools:layout_editor_absoluteY="45dp" />

<Button
        android:id="@+id/button"
        android:layout_width="107dp"
        android:layout_height="52dp"
        android:layout_marginEnd="152dp"
        android:layout_marginBottom="116dp"
        android:alpha="1"
        android:onClick="aboutclicked"
        android:text="About"
        app:layout_constraintEnd_toEndOf="parent" />

<Button
        android:id="@+id/buttonphone"
        android:layout_width="107dp"
        android:layout_height="52dp"
        android:layout_marginEnd+"152dp"
        android:layout_marginButtom="136dp"
        android:alpha="1"
        android:onClick="phoneclicked"
        android:text="Phone"
        app:layout_constrainainEnd_toEndOf="parent" />
